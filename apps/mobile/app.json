{
  "expo": {
    "name": "ShowCase",
    "slug": "showcase",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.showcase.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.showcase.app"
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "placeholder-project-id"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ShowCase to use your location to verify your activities."
        }
      ]
    ]
  }
}
